{
  "Settings": {
    "Storage": {
      "Active": "Local", // Local or S3.
      "FileNameSeparator": "_", // Better keep it like this.
      "Local": {
        "Path": "C:\\ETF\\" // Root folder for downloaded ETFs.
      },
      "S3": {
        "Path": "ETF/", // Root folder for downloaded ETFs.
        "BucketName": "etf-test",
        "Region": "us-east-1",
        "AccessKeyId": "YOUR ACCESS KEY ID",
        "SecretAccessKey": "YOUR SECRET KEY"
      }
    },
    "PostgreSQL": {
      "Address": "127.0.0.1",
      "Port": 5432,
      "User": "postgres",
      "Password": "password",
      "DatabaseName": "ETF" // Database and tables will be created automatically if they are not exists.
    },
    "iShares": {
      "Root": "https://www.ishares.com",
      "Threads": 3,
      "MaxAttempts": 2,
      "SleepSecondsBetweenRequestsMin": 3,
      "SleepSecondsBetweenRequestsMax": 10,
      "MinStocksInCsv": 5,
      "RequestTimeoutSeconds": 10,
      "FirstAvailableHoldingsDate": "2006-09-29",

      "AjaxTimestamp": 1467271812596, // Part of the URL, don't change it unless you know what are you doing.
      "Json": { // Json schema. Change it only if iShares will change Json format.
        "PropertiesQuantity": 17,
        "Ticker_Index": 0,
        "CUSIP_Index": 8,
        "ISIN_Index": 9,
        "SEDOL_Index": 10
      },
      "Csv": { // Csv schema. Change it only if iShares will change Csv format.
        "Ticker_Index": 0,
        "Name_Index": 1,
        "Sector_Index": 2,
        "AssetClass_Index": 3,
        "MarketValue_Index": 4,
        "Weight_Index": 5,
        "NotionalValue_Index": 6,
        "Shares_Index": 7,
        "Price_Index": 8,
        "Location_Index": 9,
        "Exchange_Index": 10,
        "Currency_Index": 11,
        "FXRate_Index": 12,
        "MarketCurrency_Index": 13,
        "AccrualDate_Index": 14
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
